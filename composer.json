{
  "name": "custom-d/laravel-helpers",
  "description": "A laravel helpers",
  "license": "MIT",
  "keywords": [
    "laravel",
    "helpers"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Craig Smith",
      "email": "craig.smith@customd.com"
    },
    {
      "name": "Josh Smith",
      "email": "josh@customd.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/notifications": "^8.5|^9.0|^10.0",
    "illuminate/support": "^8.5|^9.0|^10"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "nunomaduro/larastan": "^1.0|^2.0",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "CustomD\\LaravelHelpers\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CustomD\\LaravelHelpers\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit",
    "phpstan": "phpstan analyse"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CustomD\\LaravelHelpers\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
